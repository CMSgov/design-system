{
  "name": "@cmsgov/design-system",
  "version": "6.0.0",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "bin": {
    "cmsds-migrate": "./scripts/cmsds-migrate/src/main.mjs"
  },
  "description": "CMS Design System Sass/CSS and React assets",
  "repository": {
    "type": "git",
    "url": "https://github.com/CMSgov/design-system",
    "directory": "packages/design-system"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "main": "dist/react-components/cjs/index.js",
  "module": "dist/react-components/esm/index.mjs",
  "exports": {
    ".": {
        "import": "./dist/react-components/esm/index.js",
        "require": "./dist/react-components/cjs/index.js",
        "types": "./dist/react-components/types/index.d.ts"
    },
    "./react": {
        "import": "./dist/react-components/esm/index.js",
        "require": "./dist/react-components/cjs/index.js",
        "types": "./dist/react-components/types/index.d.ts"
    },
    "./preact": {
        "import": "./dist/preact-components/esm/index.js",
        "require": "./dist/preact-components/cjs/index.js",
        "types": "./dist/preact-components/types/index.d.ts"
    },
    "./web-components": {
        "import": "./dist/web-components/esm/web-components/index.js",
        "require": "./dist/web-components/cjs/web-components/index.js"
    },
    "./css/*": "./dist/css/*"
  },
  "sideEffects": ["./dist/esnext/web-components/ds-alert.js", "./dist/esnext/web-components/ds-badge.js", "./dist/esnext/web-components/ds-button.js"],
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "scripts",
    "src/components/**/*.{jsx,tsx}",
    "!src/**/*{.test,.spec,.d}.{jsx,tsx}"
  ],
  "dependencies": {
    "@popperjs/core": "^2.4.4",
    "@types/react": "^17.0.10",
    "@types/react-dom": "^17.0.10",
    "chalk": "^5.0.1",
    "classnames": "^2.2.5",
    "core-js": "^3.6.5",
    "date-fns": "^2.28.0",
    "downshift": "^3.2.10",
    "ev-emitter": "^1.1.1",
    "focus-trap-react": "^10.0.0",
    "globby": "^13.1.2",
    "inquirer": "^9.0.2",
    "lodash": "^4.17.21",
    "ora": "^6.1.2",
    "preactement": "1.8.2",
    "prop-types": "^15.8.1",
    "react-day-picker": "^8.0.5",
    "react-transition-group": "^4.4.2",
    "yargs": "^17.5.1"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  }
}
