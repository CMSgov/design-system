{
  "description": "Cleans up comments/flags made by the cmsds-migrate script.",
  "patterns": ["**/*"],
  "globbyConfig": {
    "ignore": ["**/*.json"]
  },
  "expressions": [
    { "from": "<!-- CMSDS-MIGRATE:[\\s\\S]*?-->", "to": "" },
    { "from": "\\{\\/\\* CMSDS-MIGRATE:[\\s\\S]*?\\*\\/\\}", "to": "" }
  ]
}
