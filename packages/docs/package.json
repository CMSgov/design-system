{
  "name": "@cmsgov/cms-design-system-docs",
  "version": "13.0.0",
  "private": true,
  "description": "CMS Design System Documentation",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "copyThemes": "copyfiles -u 4 ../design-system-tokens/dist/css-vars/*-theme.css ./static/themes/",
    "build": "npm run copyThemes && gatsby build",
    "clean": "gatsby clean",
    "develop": "gatsby clean && gatsby develop -H 0.0.0.0",
    "serve": "gatsby serve",
    "start": "gatsby develop",
    "typecheck": "tsc --noEmit"
  },
  "browserslist": [
    ">2% in US and not dead",
    "last 6 Chrome versions",
    "last 6 Firefox versions",
    "last 6 Edge major versions",
    "Safari >= 15",
    "iOS >= 15"
  ],
  "dependencies": {
    "@cmsgov/design-system": "13.0.0",
    "@mdx-js/mdx": "^3.1.0",
    "@mdx-js/react": "^2.3.0",
    "classnames": "^2.3.1",
    "gatsby": "5.14.0",
    "gatsby-plugin-client-side-redirect": "1.1.0",
    "gatsby-plugin-local-search": "^2.0.1",
    "gatsby-plugin-manifest": "^5.14.0",
    "gatsby-plugin-mdx": "^5.14.0",
    "gatsby-plugin-newrelic": "^2.9.0",
    "gatsby-plugin-sass": "^6.14.0",
    "gatsby-plugin-sitemap": "^6.14.0",
    "gatsby-remark-autolink-headers": "^6.14.0",
    "gatsby-source-filesystem": "^5.14.0",
    "lodash.sortby": "^4.7.0",
    "prettier": "^2.6.2",
    "prismjs": "^1.11.0",
    "react-use-flexsearch": "^0.1.1",
    "remark-gfm": "3.0.1",
    "sass": "^1.43.4",
    "url-join": "^4.0.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "copyfiles": "^2.4.1"
  }
}
