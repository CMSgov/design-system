<Layout title="React component examples">
  <div class="ds-l-container">
    <h1 class="ds-text-heading--4xl ds-u-margin-top--2">
      React component examples
    </h1>

    <AccordionExample client:visible />
    <AlertExample client:visible />
    <AutocompleteExample client:visible />
    <BadgeExample client:visible />
    <ButtonExample client:visible />
    <ChoiceListExample client:visible />
    <SingleInputDateFieldExample client:visible />
    <MultiInputDateFieldExample client:visible />
    <DropdownExample client:visible />
    <FilterChipExample client:visible />
    <LabelExample client:visible />
    <HelpDrawerExample client:visible />
    <MaskedFieldExample client:visible />
    <ModalDialogExample client:visible />
    <MonthPickerExample client:visible />
    <PaginationExample client:visible />
    <SpinnerExample client:visible />
    <TextFieldExample client:visible />
    <TableExample client:visible />
    <TabsExample client:visible />
    <TooltipExample client:visible />
    <UsaBannerExample client:visible />
    <VerticalNavigationExample client:visible />
  </div>
</Layout>